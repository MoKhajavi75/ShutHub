{
  "name": "ShutHub",
  "version": "1.0",
  "description": "This extension will hide the GitHub warn about U.S. trade controls law restrictions.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["Hide.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2
}
